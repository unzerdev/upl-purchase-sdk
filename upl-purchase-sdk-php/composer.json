{
    "name": "unzer/paylater-sdk",
    "description": "SDK to communicate with the Unzer Pay Later platform using the Unzer Pay Later Server API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Unzer Pay Later",
            "email": "integration-vie@unzer.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Unzer\\PayLater\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unzer\\PayLater\\Test\\": "tests"
        }
    },
    "require": {
        "php": "~7.2.0 || ~7.3.0 || ~7.4.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^6.5",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.6",
        "object-calisthenics/phpcs-calisthenics-rules": "^3.5",
        "slevomat/coding-standard": "^6.0",
        "squizlabs/php_codesniffer": "^3.5.0",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpstan/phpstan": "^0.12.31",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan-strict-rules": "^0.12"
    }
}
